{
  "name": "widevine-cli",
  "version": "0.1.0",
  "private": true,
  "description": "Widevine CLI",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frooastside/widevine-cli.git"
  },
  "author": "Frooastside <frooastside@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/frooastside/widevine-cli/issues"
  },
  "homepage": "https://github.com/frooastside/widevine-cli/",
  "engines": {
    "node": "^18.x.x",
    "vscode": "^1.22.0",
    "npm": "^8.x.x",
    "pnpm": "7.x.x"
  },
  "main": "dist/index.js",
  "bin": {},
  "type": "module",
  "scripts": {
    "proto:compile": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_opt=\"esModuleInterop=true\" --ts_proto_opt=\"forceLong=long\" --ts_proto_opt=\"env=node\" --ts_proto_out=. src/*.proto",
    "test": "nodemon -e ts -L -w src --exec \"pnpm build && node -r source-map-support/register dist/tests.js\"",
    "start": "node -r source-map-support/register dist/index.js",
    "watch": "nodemon -e ts -L -w src --exec \"pnpm build && pnpm start\"",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --fix --ext ts .",
    "pretty": "prettier --write .",
    "build": "tsc --incremental",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "@types/node-forge": "^1.3.1",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.136.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "holz-provider": "^0.3.1",
    "long": "^5.2.1",
    "meriyah": "^4.3.3",
    "node-fetch": "^3.3.0",
    "node-forge": "^1.3.1",
    "protobufjs": "^7.1.2",
    "puppeteer": "^19.4.1",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-adblocker": "^2.13.5",
    "puppeteer-extra-plugin-portal": "^3.1.0",
    "puppeteer-extra-plugin-recaptcha": "^3.6.6",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "uuid": "^9.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --ext ts --cache --fix",
    "*.{js,ts,css,md,json}": "prettier --write"
  }
}
