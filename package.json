{
  "name": "widevine-cli",
  "version": "0.1.0",
  "private": true,
  "description": "Widevine CLI",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frooastside/widevine-cli.git"
  },
  "author": "Frooastside <frooastside@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/frooastside/widevine-cli/issues"
  },
  "homepage": "https://github.com/frooastside/widevine-cli/",
  "engines": {
    "node": "^18.x.x",
    "vscode": "^1.22.0",
    "npm": "^8.x.x",
    "pnpm": "7.x.x"
  },
  "main": "dist/index.js",
  "bin": {},
  "type": "module",
  "scripts": {
    "start": "node -r source-map-support/register dist/index.js",
    "watch": "nodemon -e ts -L -w src --exec \"npm run build && npm run start\"",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --fix --ext ts .",
    "pretty": "prettier --write .",
    "build": "tsc --incremental",
    "clean": "rimraf dist && rimraf bin && npm run postinstall",
    "prepare": "husky install",
    "postinstall": "npm run build && node -r source-map-support/register install.js"
  },
  "devDependencies": {
    "@types/lzma-native": "^4.0.1",
    "@types/node": "^18.11.18",
    "@types/progress": "^2.0.5",
    "@types/tar-stream": "^2.2.2",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.137.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "extract-zip": "^2.0.1",
    "holz-provider": "^0.3.1",
    "long": "^5.2.1",
    "lzma-native": "^8.0.6",
    "meriyah": "^4.3.3",
    "node-fetch": "^3.3.0",
    "node-widevine": "^0.1.0",
    "progress": "^2.0.3",
    "protobufjs": "^7.1.2",
    "puppeteer": "^19.4.1",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-adblocker": "^2.13.5",
    "puppeteer-extra-plugin-portal": "^3.1.0",
    "puppeteer-extra-plugin-recaptcha": "^3.6.6",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "tar-stream": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --ext ts --cache --fix",
    "*.{js,ts,css,md,json}": "prettier --write"
  }
}
