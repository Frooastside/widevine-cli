{
  "name": "widevine-cli",
  "version": "0.1.0",
  "private": true,
  "description": "Widevine CLI",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frooastside/widevine-cli.git"
  },
  "author": "Frooastside <frooastside@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/frooastside/widevine-cli/issues"
  },
  "homepage": "https://github.com/frooastside/widevine-cli/",
  "engines": {
    "node": "^18.x.x",
    "vscode": "^1.22.0",
    "npm": "^8.x.x",
    "pnpm": "8.x.x"
  },
  "main": "dist/index.js",
  "bin": {},
  "type": "module",
  "pkg": {
    "scripts": "build/**/*.js",
    "target": "node18"
  },
  "scripts": {
    "start": "pnpm start:wakanim",
    "start:wakanim": "pnpm start:raw --force-local-drm -N 16 -o \"./{series_name}/Season {season_number}/{title} S{season_number}E{episode_number}.{ext}\" --refresh-token a4aa8049648727d5d77a237f2606790e --chrome-channel chrome --visual --verbose -i \"https://www.wakanim.tv/de/v2/catalogue/episode/7500/-dakaichi-my-number-1-omu-staffel-1-folge-1-omu\"",
    "start:aniwatch": "pnpm start:raw --force-local-drm -N 16 -o \"./{series_name}/Season {season_number}/{title} S{season_number}E{episode_number}.{ext}\" --chrome-channel chrome --visual --verbose -i \"https://aniwatch.to/watch/horimiya-the-missing-pieces-18421\"",
    "start:aniwatch:episode": "pnpm start:raw --force-local-drm -N 16 -o \"./{series_name}/Season {season_number}/{title} S{season_number}E{episode_number}.{ext}\" --chrome-channel chrome --visual --verbose -i \"https://aniwatch.to/watch/horimiya-the-missing-pieces-18421?ep=102378\"",
    "start:raw": "node -r source-map-support/register dist/index.js",
    "watch": "nodemon -e ts -L -w src --exec \"npm run build && npm run start\"",
    "testscript": "nodemon -e ts -L -w src --exec \"npm run build && node --trace-uncaught -r source-map-support/register dist/tests.js\"",
    "test": "pnpm build && ts-mocha test/**/*.spec.ts",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --fix --ext ts .",
    "pretty": "prettier --write .",
    "build": "tsc --incremental",
    "bundle": "pnpm build && esbuild dist/index.js --bundle --platform=node --target=node18 --minify --outfile=build/index.cjs",
    "package": "pnpm bundle && pkg --compression brotli --output build/wvcli build/index.cjs",
    "clean": "rimraf dist && rimraf bin && npm run postinstall",
    "prepare": "husky install",
    "postinstall": "npm run build && node -r source-map-support/register install.js"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "@types/cookie": "^0.5.1",
    "@types/crypto-js": "^4.1.1",
    "@types/koa": "^2.13.6",
    "@types/langs": "^2.0.1",
    "@types/lzma-native": "^4.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.1",
    "@types/progress": "^2.0.5",
    "@types/prompts": "^2.4.4",
    "@types/tar-stream": "^2.2.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "clone-deep": "^4.0.1",
    "esbuild": "^0.18.11",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "prettier": "3.0.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.152.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "1.0.0-rc.12",
    "commander": "^11.0.0",
    "cookie": "^0.5.0",
    "crypto-js": "^4.1.1",
    "deobfuscator": "^2.4.2",
    "dotenv": "^16.3.1",
    "enquirer": "^2.3.6",
    "extract-zip": "^2.0.1",
    "fast-xml-parser": "^4.2.5",
    "filenamify": "^6.0.0",
    "holz-provider": "^0.3.1",
    "https-proxy-agent": "^7.0.1",
    "koa": "^2.14.2",
    "langs": "^2.0.0",
    "long": "^5.2.3",
    "lzma-native": "^8.0.6",
    "meriyah": "^4.3.7",
    "node-fetch": "^3.3.1",
    "node-widevine": "^0.1.2",
    "progress": "^2.0.3",
    "protobufjs": "^7.2.4",
    "puppeteer": "^20.8.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-portal": "^3.2.1",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tar-stream": "^3.1.6",
    "uuid": "^9.0.0"
  },
  "pnpm": {
    "overrides": {
      "clone-deep": "$clone-deep"
    }
  },
  "overrides": {
    "clone-deep": "$clone-deep"
  },
  "lint-staged": {
    "*.ts": "eslint --ext ts --cache --fix",
    "*.{js,ts,css,md,json}": "prettier --write"
  }
}
